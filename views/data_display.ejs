<!doctype html>
<html>
    <head>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
        <title>Sample Data Page!</title>
    </head>
    <body>
        <h1>Projects</h1>
        <% projects.forEach(function(project) { %>
            <div class="card mt-4 p-3" style="width:60%; margin:auto">
                <h4 class="card-title"><%= project.title %></h4>
                <h5 class="card-subtitle mb-3">by <%= project.first_name %> <%= project.last_name %></h5>
                    <h6>Project Abstract:</h6>
                    <p class="card-text"><%= project.abstract %></p>
                    <a href="#" class="btn btn-primary" style="width:20%">Download Paper</a>
            </div>
        <% }); %>
    </body>
</html>
